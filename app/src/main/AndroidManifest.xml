<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amankumar.huest" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/app"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SinupActivity"
            android:label="@string/title_activity_sinup"
            android:parentActivityName=".GuestOrHostActivity" >
        </activity>
        <activity
            android:name=".GuestOrHostActivity"
            android:label="@string/title_activity_guest_or_host"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".GuestActivity"
            android:label="@string/title_activity_guest" >
        </activity>
    </application>

</manifest>
